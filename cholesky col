int cholcol(int n, double A[][nmax]) {
	int i, j, k;
	for (i = 0; i < n; i ++) {
		if (A[i][i] < E) /* <= 0 */
			return -1;
		A[i][i] = sqrt(A[i][i]);
		for (j = i + 1; j < n; j ++) {
			A[j][i] = A[j][i]/A[i][i];
			A[j][j] -= A[j][i]*A[j][i];
			for (k = i + i; k < j; k++)
				A[j][k] -= A[j][i]*A[k][i];
		}
	}
	return 0;
}
